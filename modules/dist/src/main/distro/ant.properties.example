#
# A sample ant properties file
#

# Optional JBoss Home
jboss600.home=@jboss600.home@
jboss601.home=@jboss601.home@
jboss700.home=@jboss700.home@

# The JBoss server under test. This can be [jboss600|jboss601|jboss700]
jbossws.integration.target=jboss601

# The JBoss settings
jboss.server.instance=default
jboss.bind.address=localhost

# JBoss JMX invoker authentication
#jmx.authentication.username=admin
#jmx.authentication.password=admin

# Java Compiler options
javac.debug=yes
javac.deprecation=no
javac.fail.onerror=yes
javac.verbose=no
